{
	"plugins": ["plugins/markdown"],
	"source": {
		"include": ["src/"],
		"includePattern": ".js$",
		"excludePattern": "(node_modules/|docs)_"
	},
	"sourceType": "module",
	"tags": {
		"allowUnknownTags": true,
		"dictionaries": ["jsdoc", "closure"]
	},
	"templates": {
		"cleverLinks": true,
		"monospaceLinks": false
	},
	"opts": {
		"encoding": "utf8",
		"destination": "./docs/",
		"recurse": true
	}
}
